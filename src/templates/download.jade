| {% extends "page.html" %}

| {% block style %}
style.
	h2 {
		margin-top: 3ex;
	}
	h2 .version {
		font-size: small;
		margin-left: 1ex;
		vertical-align: top;
	}
	svg {
		width: 100%;
		height: auto;
	}
	a.downloadbutton {
		display: inline-block;
		border: 3px solid #f88;
		background-color: #fffceb;
		padding: 2ex 3ex 1ex 3ex;
		border-radius: 1ex;
		box-shadow: 0.5ex 0.5ex 1ex rgba(0, 0, 0, 0.25);
		margin: 2ex;
		color: black;
	}
	a.downloadbutton:hover {
		box-shadow: 0.75ex 0.75ex 1.5ex rgba(0, 0, 0, 0.5);
	}
	.downloadbutton p {
		font-size: small;
		text-align: center;
		margin: 1ex 0 0 0;
	}
| {% endblock %}

